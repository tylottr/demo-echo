{
  "name": "demo-echo",
  "version": "1.0.0",
  "description": "Simple Express app that echos the first value from path, the message query string value, or a default message. A health endpoint is at /health and /404 or /fail returns a 404.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start:debug": "DEBUG=* node --inspect-brk=9229 app.js",
    "test": "mocha --colors --exit --recursive ./tests",
    "test:ci": "mocha --exit --reporter mocha-junit-reporter --recursive ./tests",
    "lint": "eslint . --ext .js",
    "lint:ci": "eslint --format junit --output-file lint.xml . --ext .js"
  },
  "keywords": [],
  "author": "Tyla Boff",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^6.2.2",
    "mocha": "^7.0.0",
    "mocha-junit-reporter": "^1.23.3",
    "supertest": "^4.0.2"
  }
}
